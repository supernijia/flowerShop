
.content {
		width: 100%;
		height: auto;
}
.items {
		display: flex;
		flex-wrap: wrap;
		justify-content: space-around;
		margin-top: 20rpx;
}
.items>view {
		margin-bottom: 60rpx;
		box-shadow: 0rpx 1rpx 0rpx 0rpx rgb(241, 241, 241);
}
.CommodityName {
		font-size: 28rpx;
		display: block;
		width: 100%;
		margin-top: 10rpx;
		font-weight: 200;
}
.Price {
		font-size: 25rpx;
		color: red;
}
.img {
		width: 300rpx;
		height: 300rpx;
}
.img>image {
		width: 100%;
		height: 100%;
}
